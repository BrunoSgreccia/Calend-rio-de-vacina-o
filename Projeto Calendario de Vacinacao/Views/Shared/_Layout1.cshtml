    <script src=@Url.Content("~/Content/js/grayscale.min.js")></script>
    <script src=@Url.Content("~/public/plugins/datatables/jquery.dataTables.min.js")></script>
    <script src=@Url.Content("~/public/plugins/datatables/dataTables.bootstrap.min.js")></script>
    @RenderSection("scripts", required: false)





</body>
</html>
