<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    @RenderSection("head", required: false)

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Bem-Vindo ao Calendario de Vacinação Infantil!</title>
    <!-- Bootstrap core CSS -->
    <link href=@Url.Content("~/Content/vendor/bootstrap/css/bootstrap.min.css") rel="stylesheet">
    <!-- Custom fonts for this template -->
    <link href=@Url.Content("~/Content/vendor/font-awesome/css/font-awesome.min.css") rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lora:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href='https://fonts.googleapis.com/css?family=Cabin:700' rel='stylesheet' type='text/css'>
    <!-- Custom styles for this template -->
    @*<link href=@Url.Content("~/Content/css/grayscale.min.css") rel="stylesheet">*@
    <link href=@Url.Content("~/Content/css/grayscale.css") rel="stylesheet" />
    <link rel="stylesheet" href=@Url.Content("~/public/plugins/datatables/dataTables.bootstrap.css")>
</head>
<body id="page-top">
